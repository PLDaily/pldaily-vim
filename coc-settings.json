{
  "tslint.autoFixOnSave": true,
  "tsserver.enableJavascript": true,
  "eslint.autoFixOnSave": true,
  "git.enableGutters": true,
  "git.addedSign.hlGroup": "GitGutterAdd",
  "git.changedSign.hlGroup": "GitGutterChange",
  "git.removedSign.hlGroup": "GitGutterDelete",
  "git.topRemovedSign.hlGroup": "GitGutterDelete",
  "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",
  "git.addGBlameToVirtualText": true,
  "git.showCommitInFloating": true,
  "git.semanticCommit.scope": false,
  "translator.engines": ["youdao"],
  "list.source.files.args": ["--hidden", "--files"],
  "list.source.files.excludePatterns": ["**/.git/**/*"],
  "list.alignColumns": false,
  "list.previewSplitRight": false,
  "codeLens.enable": true,
  "codeLens.position": "eol",
  "semanticTokens.filetypes": ["*"],
  "tree.key.toggle": "o",
  "outline.sortBy": "position",
  "suggest.selection": "recentlyUsed",
  "suggest.enablePreselect": false,
  "suggest.enablePreview": true,
  "suggest.echodocSupport": false,
  "suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"constructor": "\uf0ad",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
  "html.filetypes": ["typescriptreact", "html"],
  "smartf.timeout": 3000,
  "smartf.jumpOnTrigger": false,
  "smartf.wordJump": false,
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "imselect.enableStatusItem": false,
  "imselect.enableFloating": false,
  "diagnostic.enable": true,
  "diagnostic.warningSign": "⚠",
  "diagnostic.errorSign": "×",
  "java.format.enabled": true,
  "java.format.settings.url": "/Users/pengchangdong/.config/coc/extensions/node_modules/redhat.java/eclipse-formatter.xml",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "clojure-lsp": {
      "command": "bash",
      "args": ["-c", "clojure-lsp"],
      "filetypes": ["clojure"],
      "rootPatterns": ["project.clj", "deps.edn"],
      "additionalSchemes": ["jar", "zipfile"],
      "trace.server": "verbose",
      "initializationOptions": {
      }
    }
  },
  "coc.preferences.extensionUpdateCheck": "daily"
}
